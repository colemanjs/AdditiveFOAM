FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      heatSourceProperties;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

sources (beam);

beam
{
    pathName            scanPath;    

    absorptionModel     constant;
    
    constantCoeffs
    {
        eta             0.33;
    }

    heatSourceModel     Gaussian;
    
    GaussianCoeffs
    {
        dimensions      (85.0e-6 85.0e-6 50e-6);
        nPoints         (2 2 2);
    }
}

