{
    features        ( );
    refinementSurfaces
    {
    }
    refinementRegions
    {
        refinementBox
        {
            mode            inside;
            levels          ( ( 3 3 ) );
        }
    }
    nCellsBetweenLevels 3;
    locationInMesh  ( 0 0 0 );
}

snapControls
{
}

addLayersControls
{
    layers
    {
    }
    relativeSizes   true;
    expansionRatio  1.2;
    finalLayerThickness 0.5;
    minThickness    0.001;
}

mergeTolerance  1e-08;


